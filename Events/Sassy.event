
//Label Press Suspend

//ORG $5920C0
//EVBIT_MODIFY 0x4
//TEXTSHOW 0x8FE
//TEXTEND
//SVAL 0x7 0x1
//BNE 0x0 0xC 0x7
//ASMC 0xB5D5D
//TEXTSHOW2 0x8FF
//TEXTEND
//MUSCMID 0x7FFF
//FADI 4
//MNTS 0x0
//LABEL 0x0
//REMA
//EVBIT_T 0x7
//ENDA

PUSH
ORG 0xD38C
POIN SassyTextEvents
POP

SassyTextEvents:
EVBIT_MODIFY 0x4
RANDOMNUMBER 4
BEQ 0x0 0x0 0xC
SVAL 0x1 0
BEQ 0x1 0x1 0xC
SVAL 0x1 2
BEQ 0x2 0x1 0xC
SVAL 0x1 3
BEQ 0x3 0x1 0xC
SVAL 0x1 4
BEQ 0x4 0x1 0xC

LABEL 0x0
TEXTSHOW SassyText1
TEXTEND
GOTO 0x6

LABEL 0x1
TEXTSHOW SassyText2
TEXTEND
GOTO 0x6

LABEL 0x2
TEXTSHOW SassyText3
TEXTEND
GOTO 0x6

LABEL 0x3
TEXTSHOW SassyText4
TEXTEND
GOTO 0x6

LABEL 0x4
TEXTSHOW SassyText5
TEXTEND
GOTO 0x6


LABEL 0x6
SVAL 0x1 1
BNE 0x5 0x1 0xC
ASMC 0xB5D5D
TEXTSHOW2 SassyTextEnd
TEXTEND
MUSCMID 0x7FFF
FADI 4
MNTS 0x0

LABEL 0x5
REMA
NoFade
ENDA

#ifdef __DEBUG__
//Label Win Button
	WinButtonEvent:
	EVBIT_MODIFY 0x4
	TEXTSHOW WinButtonNarratorText
	TEXTEND
	REMA
	SVAL 0x1 1
	BNE 0x0 0x1 0xC
		// They selected "yes."
		ASMC GetChapterNumber|1
		SVAL 0x1 MercenaryMilitia2
		BNE 0x4 0x1 0xC
			ASMC MercenaryMilitia2DebugMenu|1
			BEQ 0x1 0x0 0xC // Go to regular (just run EndingScene)
				ASMC Win_Effect_Special|1
				GOTO 0x0
		LABEL 0x4
		SVAL 0x1 MercenaryBandit4
		BNE 0x5 0x1 0xC
			ASMC MercenaryBandit4DebugMenu|1	
			BEQ 0x1 0x0 0xC // Go to regular (just run EndingScene)
				ASMC Win_Effect_Special|1
				GOTO 0x0
		LABEL 0x5
		SVAL 0x1 MilitaryPass4
		BNE 0x6 0x1 0xC
			ASMC MilitaryPass4DebugMenu|1
			BEQ 0x1 0x0 0xC // Go to regular (just run EndingScene)
				ASMC Win_Effect_Special|1
				GOTO 0x0
		LABEL 0x6
		SVAL 0x1 MilitaryPassAltia7
		BNE 0x7 0x1 0xC
			ASMC MilitaryPassAltia7DebugMenu|1 // Because this one acutally has 3 endings
			BEQ 0x1 0x0 0xC // Go to regular (EndingScene should have Izuk is alive and did not escape)
				SVAL 0x2 1
				BNE 0x8 0x2 0xC
					// Go to Escape
					ASMC Win_Effect_Special|1
					GOTO 0x0
				LABEL 0x8
					// Go to Izuk dies
					ASMC Win_Effect_Special_2|1
					GOTO 0x0
		LABEL 0x7
		SVAL 0x1 MercenaryMilitiaKill7
		BEQ 0x3 0x1 0xC
		SVAL 0x1 MercenaryMilitiaWaitCesaria7
		BEQ 0x3 0x1 0xC
		SVAL 0x1 MercenaryMilitiaWaitArcanae7
		BEQ 0x3 0x1 0xC
		SVAL 0x1 MercenaryBanditBandit7
		BEQ 0x3 0x1 0xC
		SVAL 0x1 MercenaryBanditDisband7
		BEQ 0x3 0x1 0xC
		
		SVAL 0x1 MilitaryPassHaedric7
		BEQ 0x3 0x1 0xC
		GOTO 0x1
		
			LABEL 0x3
			ASMC GoodBadEndingDebugMenu|1
			BEQ 0x2 0x0 0xC
				ASMC Win_Effect_Bad_Ending|1
				GOTO 0x0
			LABEL 0x2
				ASMC Win_Effect_Special|1 // This was gonna be Win_Effect_Good_Ending, but it would have done the exact same as Win_Effect_Special
				GOTO 0x0
	LABEL 0x1
		ASMC Win_Effect|1 // There is no alternate ending to test. Just go to EndingScene
	LABEL 0x0
	NoFade
	ENDA

